
#include<stdio.h>
#include<stdlib.h>

int main (){
	
	int vect[5] = {99, 88, 2, 5, 0};
	int aux, min;
	
	for (int i = 0; i < 5; i ++){
		min = i;
		for (int j = 0; j < 5; j ++){
			if (vect[j] < vect[min]){
				min = j;
			}
		}
		aux = vect[i];
		vect[i] = vect[min];
		vect[min] = aux;
	}
	printf("VECTOR ORDENADO \n");
	for (int i = 0; i < 5; i ++){
		printf("[%i]", vect[i]);
	}
	
}
