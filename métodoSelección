
#include<stdio.h>
#include<stdlib.h>

int main (){
	int vect[5] = {99, 88, 2, 5, 0};
	//ordenar de mayor a menor, recorrer el vector y cuando encuentre un n√∫mero menor a ese mismo lo sustituye
	
	for (int i = 0; i < 5; i ++ ){// ciclo que recorre todo el vector
		//for (int j = 0; j < 5; j ++){ //ciclo 
				int aux;
				aux = vect[i];
				if (aux < vect[i+1]){
					vect[i] = aux;
			 }
			 printf("[%i]", vect[i]);
			}
			
	
		//}
}
